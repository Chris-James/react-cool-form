{
  "name": "rcf-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "link-pkg": "yarn link 'react-cool-form'",
    "start:auto": "react-scripts start",
    "start:dev": "REACT_APP_ENV=dev react-scripts start",
    "start:prod": "yarn build && serve -s build",
    "build": "react-scripts build",
    "lint": "run-s lint:*",
    "lint:code": "eslint --fix . --ext .js,.ts,.tsx",
    "lint:type": "tsc",
    "lint:style": "stylelint --fix \"**/*.{css,ts,tsx}\"",
    "lint:format": "prettier -w . -u --loglevel silent",
    "test": "react-scripts test --passWithNoTests",
    "test:cov": "yarn test --coverage --watchAll=false",
    "clean": "run-p clean:*",
    "clean:build": "rimraf build",
    "clean:cov": "rimraf coverage",
    "eject": "react-scripts eject"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn test --watchAll=false"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": "eslint --fix",
    "*.{css,ts,tsx}": "stylelint --fix",
    "**/*": "prettier -w -u"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.1.2",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^12.5.0",
    "@types/jest": "^26.0.16",
    "@types/node": "^14.14.10",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/yup": "^0.29.10",
    "normalize.css": "^8.0.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-scripts": "4.0.1",
    "typescript": "~4.1.2",
    "yup": "^0.32.1"
  },
  "devDependencies": {
    "eslint-config-welly": "^1.8.5",
    "husky": "^4.3.4",
    "lint-staged": "^10.5.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "serve": "^11.3.2",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0"
  },
  "resolutions": {
    "postcss-safe-parser": "^4.0.2"
  }
}
